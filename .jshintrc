{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "newcap": true,
    "noarg": true,
    "nonbsp": true,
    "nonew": true,
    "undef": true,
    "strict": true,
    "trailing": true,
    "indent": 2,
    "maxlen": 120,
    "latedef": "nofunc",
    "quotmark": "single",
    "unused": true
}

// {
//   "bitwise": true,      // Prohibit the use of bitwise operators
